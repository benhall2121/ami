

<table class="transaction_table">
  <tr class="header">
    <td>Amount</td>
    <td>Type</td>
    <td>Startup Name</td>
    <td>Date</td>
  </tr>

  <% @transactions.each do |t| %>
  <tr>
    <td>$<%= t.amount %></td>
    <td><%= t.transaction_type %></td>
    <td><%= t.startup.startup_name %></td>
    <td><%= t.created_at.strftime('%m/%d/%Y') %></td>
  </tr>
  <% end %>

</table>