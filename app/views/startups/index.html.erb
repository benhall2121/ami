
<div class="page_nav_outer_wrapper">
  <div class="page_nav_inner_wrapper main_width">
    <div class="page_nav_title">
      INITIATIVES
    </div>
  </div>
</div>

<div class="page_content_wrapper main_width">
<% if is_admin? %>
  <div class="new_wrapper">
    <%= link_to 'New Startup', new_startup_path %>
  </div>
<% end %>

<%= form_tag startups_path, :method => 'get', :id => "startups_search" do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", :name => nil %>
  </p>
  <div id="startups">
    <%= render 'startups' %>
  </div>
<% end %>
</div>



